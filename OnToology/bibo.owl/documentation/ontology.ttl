      rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
      rdfs:label "Thesis"@en ;
      rdfs:subClassOf bibo:Document ;
      ns:term_status "stable" .

bibo:owner
