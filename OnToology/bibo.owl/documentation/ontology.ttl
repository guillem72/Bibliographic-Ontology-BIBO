              [ a       owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty terms:hasPart
              ] ;
      ns:term_status "stable" .

bibo:handle
      a       owl:DatatypeProperty ;
